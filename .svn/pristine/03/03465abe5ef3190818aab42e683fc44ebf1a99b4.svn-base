		div.row#bildergallerie
			div.row
				div.col-md-12
					h2 letzte Aktivitäten
					ul.pagination
						if page == 1
							li.disabled
								a(href="/") &laquo;
						else
							li
								a(href="/") &laquo;
						- for (var i = 0; i < pageCount; ++i) {
							if i+1 == page
								li.active
									a(href="/page/#{i+1}") #{i+1}
							else
								li
									a(href="/page/#{i+1}") #{i+1}
						- }
						if page == maxPage
							li.disabled
								a(href="/page/#{maxPage}") &raquo;
						else
							li
								a(href="/page/#{maxPage}") &raquo;
			div#images.row
				//div.col-md-12
				each picture in actImages
					- var date = new Date(picture.time_stamp)
					div.col-lg-3.col-md-4.col-xs-6.thumbnail
						a(rel="imagegroup", href="/public/nistkasten/normal/#{picture.filename}", title="#{date.getDate()}.#{date.getMonth()+1}.#{date.getFullYear()} um #{date.getHours()}:#{date.getMinutes()} Uhr")
							img(src="/public/nistkasten/thumbs/#{picture.filename}").img-responsive
						div.caption
							div	#{date.getDate()}.#{date.getMonth()+1}.#{date.getFullYear()} um #{date.getHours()}:#{date.getMinutes()} Uhr
								a(onclick="alert('speichern');return false;", href="#")
									if pictureSaved
										span(style="float:right").glyphicon.glyphicon-floppy-saved
									else
										span(style="float:right").glyphicon.glyphicon-floppy-disk
			div.row
				div.col-md-12
					ul.pagination
						if page == 1
							li.disabled
								a(href="/") &laquo;
						else
							li
								a(href="/") &laquo;
						- for (var i = 0; i < pageCount; ++i) {
							if i+1 == page
								li.active
									a(href="/page/#{i+1}") #{i+1}
							else
								li
									a(href="/page/#{i+1}") #{i+1}
						- }
						if page == maxPage
							li.disabled
								a(href="/page/#{maxPage}") &raquo;
						else
							li
								a(href="/page/#{maxPage}") &raquo;


